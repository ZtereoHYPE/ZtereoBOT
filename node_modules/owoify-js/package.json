{
  "name": "owoify-js",
  "version": "1.0.7",
  "description": "owoifier: JS Boogaloo Edition",
  "keywords": [
    "owo",
    "owoifier",
    "hell"
  ],
  "repository": "git@github.com:mohan-cao/owoify-js.git",
  "bugs": {
    "url": "https://github.com/mohan-cao/owoify-js/issues"
  },
  "homepage": "https://github.com/mohan-cao/owoify-js/",
  "private": false,
  "license": "MIT",
  "author": "mohan-cao <mohancao@yahoo.com.au>",
  "main": "dist/index.js",
  "module": "dist/owoify-js.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --format cjs,esm,umd",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "husky": "^4.0.10",
    "tsdx": "^0.12.3",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  }
}
